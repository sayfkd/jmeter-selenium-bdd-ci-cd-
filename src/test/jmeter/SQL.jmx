        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Requête JDBC" enabled="true">
          <stringProp name="dataSource">myDb</stringProp>
          <stringProp name="query">SELECT 
    e.emp_no,
    e.first_name,
    e.last_name,
    e.gender,
    e.hire_date,
    d.dept_name,
    dm.emp_no AS manager_no,
    m.first_name AS manager_first_name,
    m.last_name AS manager_last_name,
    de.from_date AS emp_from_date,
    de.to_date AS emp_to_date,
    dm.from_date AS manager_from_date,
    dm.to_date AS manager_to_date
FROM 
    employees e
JOIN 
    dept_emp de ON e.emp_no = de.emp_no
JOIN 
    departments d ON de.dept_no = d.dept_no
LEFT JOIN 
    dept_manager dm ON d.dept_no = dm.dept_no
LEFT JOIN 
    employees m ON dm.emp_no = m.emp_no
WHERE 
    CURRENT_DATE BETWEEN de.from_date AND de.to_date
    AND CURRENT_DATE BETWEEN dm.from_date AND dm.to_date;
</stringProp>
