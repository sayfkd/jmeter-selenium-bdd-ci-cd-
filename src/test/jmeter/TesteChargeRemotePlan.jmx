          <stringProp name="WebDriverSampler.script">WDS.sampleResult.sampleStart()
WDS.browser = new org.openqa.selenium.chrome.ChromeDriver()
WDS.browser.get(&quot;https://www.saucedemo.com/&quot;)

var usernameInput = WDS.browser.findElement(org.openqa.selenium.By.id(&quot;user-name&quot;))
usernameInput.sendKeys(&quot;standard_user&quot;)

var passwordInput = WDS.browser.findElement(org.openqa.selenium.By.id(&quot;password&quot;))
passwordInput.sendKeys(&quot;secret_sauce&quot;)


var loginButton = WDS.browser.findElement(org.openqa.selenium.By.id(&quot;login-button&quot;))
loginButton.click()

WDS.browser.close()

var productLabel = WDS.browser.findElement(org.openqa.selenium.By.className(&quot;title&quot;))
if (productLabel.getText() === &quot;Products&quot;) {
    WDS.log.info(&quot;Connexion reussite&quot;)
} else {
    WDS.log.error(&quot;Erreur&quot;)
}

WDS.sampleResult.sampleEnd()</stringProp>
